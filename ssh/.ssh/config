ServerAliveInterval 300

Host *
    ForwardAgent yes

Host github-kumaresan-plivo
    User kumaresan-plivo
    HostName github.com
    IdentityFile ~/.ssh/id_rsa
    IdentitiesOnly yes

Host github-krajeswaran
    User krajeswaran
    HostName github.com
    IdentityFile ~/.ssh/id_rsa_github
    IdentitiesOnly yes

Host gitlab.com
    User kumaresan-rajeswaran
    IdentityFile ~/.ssh/id_rsa_github
    IdentitiesOnly yes
    PreferredAuthentications publickey

Host staging-callular-*.private.plivops.com
    User kumaresan
    IdentityFile ~/.ssh/id_rsa
    ServerAliveInterval 120
    Compression yes
    StrictHostKeyChecking no
    CheckHostIP no
    UserKnownHostsFile /dev/null
    LogLevel ERROR
    ForwardAgent yes
    ForwardX11 no
    ControlMaster auto
    ControlPath /tmp/%r@%h:%p
    ProxyCommand ssh -l kumaresan staging-callular-jumpbox1.plivops.com -W %h:%p


Host staging-api-*.private.plivops.com
    User kumaresan
    IdentityFile ~/.ssh/id_rsa
    ServerAliveInterval 120
    Compression yes
    StrictHostKeyChecking no
    CheckHostIP no
    UserKnownHostsFile /dev/null
    LogLevel ERROR
    ForwardAgent yes
    ForwardX11 no
    ControlMaster auto
    ControlPath /tmp/%r@%h:%p
    ProxyCommand ssh -l kumaresan staging-api-jumpbox1.plivops.com -W %h:%p
